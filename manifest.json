{
    "name": "Glists",
    "version": "0.1",
    "description": "An annoyingly simple checklists app, using on Google tasks",
    "app": {
        "launch": {
            "local_path": "index.html"
        }
    },
    "content_scripts": [{
        "matches": ["https://www.google.com/robots.txt"],
        "js": ["auth/oauth_inject.js"],
        "run_at": "document_start"
    }],
    "permissions": ["https://accounts.google.com/o/oauth2/token"],
    "icons": {
        "16": "assets/icon-16.png",
        "48": "assets/icon-48.png",
        "128": "assets/icon-128.png"
    },
    "manifest_version": 2
}
