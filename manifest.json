{
    "name": "Glists Dev",
    "version": "0.2",
    "description": "An annoyingly simple checklists app, using on Google tasks",
    "app": {
        "launch": {
            "local_path": "index.html"
        }
    },
    "content_scripts": [{
        "matches": ["https://www.google.com/robots.txt"],
        "js": ["auth/oauth_inject.js"],
        "all_frames": true,
        "run_at": "document_start"
    }],
    "permissions": ["https://accounts.google.com/o/oauth2/token"],
    "content_security_policy":
        "script-src 'self' https://platform.twitter.com https://apis.google.com https://api.flattr.com; object-src 'self'",
    "icons": {
        "16": "meta/logo/icon-16.png",
        "48": "meta/logo/icon-48.png",
        "128": "meta/logo/icon-128.png"
    },
    "manifest_version": 2
}
